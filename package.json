	"devDependencies": {
		"@types/node": "^20.10.0",
		"@typescript-eslint/eslint-plugin": "^6.12.0",
		"@typescript-eslint/parser": "^6.12.0",
		"eslint": "^8.54.0",
		"eslint-plugin-prettier": "^5.0.1",
		"husky": "^8.0.3",
		"lint-staged": "^15.1.0",
		"prettier": "^3.1.0",
		"typescript": "^5.3.2"
	},
	"dependencies": {
		"axios": "^1.6.2"
	}
